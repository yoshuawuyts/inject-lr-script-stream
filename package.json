{
  "name": "inject-lr-script-stream",
  "version": "1.0.0",
  "description": "Streamingly inject a livereload script into html",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "deps:pkg": "ncu",
    "deps:update": "ncu -a",
    "test": "standard && npm run deps && NODE_ENV=test node test",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover test.js"
  },
  "repository": "yoshuawuyts/inject-lr-script-stream",
  "keywords": [
    "stream",
    "inject",
    "livereload",
    "html",
    "watch"
  ],
  "license": "MIT",
  "dependencies": {
    "hyperstream": "^1.2.2"
  },
  "devDependencies": {
    "concat-stream": "^1.5.0",
    "dependency-check": "^2.5.0",
    "from2-string": "^1.1.0",
    "istanbul": "^0.3.19",
    "standard": "^5.2.2",
    "tape": "^4.2.0"
  },
  "files": [
    "index.js",
    "bin/*"
  ]
}
